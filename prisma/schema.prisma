generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model txnData {
  id              String   @id @default(cuid())
  txnId           String   @unique
  roomId          String
  privyId         String
  txnStepNo       Int
  firstTxnHash    String
  firstTxnStatus  String
  firstTxnEstTime DateTime
  secondTxnData   Json
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
